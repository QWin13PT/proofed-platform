# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# YouTube OAuth (Google Cloud Console)
# Get credentials at: https://console.cloud.google.com/apis/credentials
YOUTUBE_CLIENT_ID=your_youtube_client_id_here
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret_here

# Stripe OAuth (Stripe Dashboard)
# Get credentials at: https://dashboard.stripe.com/settings/applications
STRIPE_CLIENT_ID=your_stripe_client_id_here
STRIPE_CLIENT_SECRET=your_stripe_client_secret_here

# Google Analytics OAuth (optional)
GOOGLE_ANALYTICS_CLIENT_ID=your_ga_client_id_here
GOOGLE_ANALYTICS_CLIENT_SECRET=your_ga_client_secret_here

# HubSpot OAuth (optional)
HUBSPOT_CLIENT_ID=your_hubspot_client_id_here
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret_here

# zkVerify Configuration
NEXT_PUBLIC_ZKVERIFY_ENDPOINT=wss://testnet-rpc.zkverify.io
NEXT_PUBLIC_ZKVERIFY_NETWORK=testnet
ZKVERIFY_SEED_PHRASE=your_seed_phrase_here_with_12_or_24_words

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

